# Remove symlink that was created in steps/glibc-2-a (?)
lfs_log "before deletion of symlink ls -ld /usr/lib/gcc = $(ls -ld /usr/lib/gcc)"
rm -f /usr/lib/gcc
lfs_log "after  deletion of symlink ls -ld /usr/lib/gcc = $(ls -ld /usr/lib/gcc)"

